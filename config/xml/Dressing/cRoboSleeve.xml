<Controller xmlns:xi="http://www.w3.org/2003/XInclude"
	    usage="
		   &#xA;  Configuration file to illustrate sleeving on a shirt sleeve over an arm.
		   &#xA;  The Gui sliders allow to drag the sleeve with the Task::Sleeve's first
		   &#xA;  slider. A value of 0 will drag it to the shoulder, a value of 1 will
		   &#xA;  drag it to the wrist. The second slider component is the lateral offset
		   &#xA;  of the plane spanned by shoulder-elbow-wrist. The third one is the 
		   &#xA;  closest distance of the drag point to the arm's shapes.
		   &#xA;  
		   &#xA;  Here is how to start an example:
		   &#xA;  
		   &#xA;  bin/Rcs -m 5 -physicsEngine SoftBullet -dir config/xml/Examples/ -f cRoboSleeve.xml -physics -noHud
		   &#xA;  
		   &#xA;  Then, press: k - T - T - T - P - g - C
		   &#xA;  
		   " >
  

  <Graph resourcePath="${SIT}/Data/RobotMeshes/1.0/data" name="DefaultPose" >

    <Body name="Ground Plane" >
      <Shape type="BOX" extents="6.0 6.0 0.04" transform="0 0 -0.02 0 0 0" 
             color="PEWTER" graphics="true" />
      <Shape type="FRAME" scale="1" />
    </Body>

    <Group prev="Ground Plane" transform="0 0 0 0 0 0" >
      <xi:include href="gSleeve.xml" />
    </Group >

    
    <Body name="Table" prev="Ground Plane" transform="0.6 -0.6 0.5 0 0 0" >
      <Shape type="BOX" extents="1.0 0.5 0.02" graphics="true" transform="0 0 -0.01 0 0 0" />
      <Shape type="FRAME" scale="0.25" />
    </Body>
 
    
    <Group name="_L" prev="Table" transform="-0.3 0 0 0 0 180" > 
      <xi:include href="../Kinova/j2s7s300.xml" /> 
    </Group > 

    <Group name="_R" prev="Table" transform="0.3 0 0 0 0 180" > 
      <xi:include href="../Kinova/j2s7s300.xml" /> 
    </Group > 
    
    <Body name="Grip_L" prev="j2s7s300_end_effector_L" transform="0 0 0 90 0 90" >
      <Shape type="SSL" radius="0.025" length="0.08" graphics="true" transform="0 0 -0.08 0 0 0" />
      <Shape type="FRAME" scale="0.25" />
    </Body>
    
    <Body name="Grip_R" prev="j2s7s300_end_effector_R" transform="0 0 0 90 0 90" >
      <Shape type="FRAME" scale="0.25" />
    </Body>

    <Group prev="Ground Plane" transform="0 0 0 0 0 0" >
      <xi:include href="gHumanModel.xml" />
    </Group >

