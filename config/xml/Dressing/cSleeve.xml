<Controller
usage="
&#xA;  Configuration file to illustrate sleeving on a shirt sleeve over an arm.
&#xA;  The Gui sliders allow to drag the sleeve with the Task::Sleeve's first
&#xA;  slider. A value of 0 will drag it to the shoulder, a value of 1 will
&#xA;  drag it to the wrist. The second slider component is the lateral offset
&#xA;  of the plane spanned by shoulder-elbow-wrist. The third one is the 
&#xA;  closest distance of the drag point to the arm's shapes.
&#xA;  
&#xA;  Here is how to start an example:
&#xA;  
&#xA;  bin/Rcs -m 5 -physicsEngine SoftBullet -dir config/xml/Examples/ -f cSleeve-nice.xml -physics -noHud
&#xA;  
&#xA;  Then, press: k - T - T - T - P - g - C
&#xA;  
" >
  

  <Graph>

    <Body name="Ground Plane" >
      <Shape type="BOX" extents="6.0 6.0 0.04" transform="0 0 -0.02 0 0 0" 
             color="PEWTER" graphics="true" />
      <Shape type="FRAME" scale="1" />
    </Body>

    <Body name="Body" rigid_body_joints="0 0 1.5 0 0 0" physics="kinematic" mass=".1" >
      <Shape type="SPHERE" color="PEWTER" radius="0.1" graphics="true" material="object" />
    </Body>

    <Body name="UpperArm" prev="Body" physics="dynamic" mass=".1" >
      <Joint name="shoulder" type="RotY" range="-170 150 170" />
      <Shape type="SSL" color="RED" radius="0.06" length="0.9" physics="false" distance="true" graphics="true" />
      <Shape type="SSL" color="RED_TRANS" radius="0.08" length="0.9" graphics="false" distance="false" material="object" />
      <Shape type="FRAME" scale="0.5" />
    </Body>

    <Body name="ForeArm" prev="UpperArm" physics="dynamic" mass=".1" >
      <Joint name="elbow" type="RotY" range="-170 -60 170" transform="0 0 0.9 0 0 0" />
      <Shape type="SSL" color="GREEN" radius="0.06" length="0.9" physics="false" graphics="true" distance="true" />
      <Shape type="SSL" color="GREEN_TRANS" radius="0.08" length="0.9" graphics="false" distance="false" material="object" />
      <Shape type="FRAME" scale="0.5" />
    </Body>

    <Body name="Hand" prev="ForeArm" physics="kinematic" mass=".1" >
      <Joint type="RotY" range="-170 0 170" transform="0 0 0.9 0 0 0" />
      <Shape type="SPHERE" color="BLUE" radius="0.07" physics="false" graphics="true" />
      <Shape type="SPHERE" color="BLUE_TRANS" radius="0.09" graphics="false" material="object" />
      <Shape type="FRAME" scale="0.5" />
    </Body>

    <Body name="Slider" physics="kinematic" mass="0.1" transform="0 0 -0.1644 0 -90 0">
      <Joint name="SliderX" type="TransX" range="-1 0.8 1" weightJL="0" />
      <Joint name="SliderY" type="TransY" range="-1 0 1" weightJL="0" />
      <Joint name="SliderZ" type="TransZ" range="-1 0.72 2" weightJL="0" />
      <Shape type="SPHERE" color="GREEN" radius="0.005" distance="true" graphics="true" />
    </Body>

    <Body name="Slider_A" prev="Slider" physics="fixed" mass="0.1" transform="0 0 0 0 0 0" >
      <Shape type="SPHERE" color="GREEN" radius="0.005" distance="true" graphics="true" />
      <Shape type="FRAME" scale="0.1" />
    </Body>

    <Body name="Slider_B" prev="Slider_A" physics="dynamic" mass="0.1" transform="0 0 -0.01 0 0 0" >
      <Joint name="SliderThY" type="RotY" range="-360 0 360" weightJL="0" constraint="true" torqueLimit="0" />
      <Shape type="SSL" color="RED" radius="0.005" length="0.06" transform="0 0 0 0 90 0" distance="true" graphics="true" />
      <Shape type="FRAME" scale="0.1" />
    </Body>

    <Body name="SleeveAnchor" prev="Slider_B" transform="0.1644 0 0.02 0 0 0" physics="dynamic" mass=".1" >
      <Shape type="TORUS" color="GREEN" radius="0.1" length="0.02" graphics="true" material="object" transform="0 0 -0.02 0 0 0" />
    </Body>

    <Body name="Sleeve" prev="SleeveAnchor" physics="fixed" transform="0 0 -0.3 0 0 0" mass=".1" >
      <Shape type="MESH" meshFile="sleeve.stl" softPhysics="true" material="cloth3" />
      <Shape type="FRAME" scale="0.3" />
    </Body>







